{
  "manifest": {
    "displayName": "Room Number",
    "invocationName": "Room Number",
    "category": "PRODUCTIVITY"
  },
  "actions": [
    {
      "name": "com.acme.actions.Room_Number",
      "availability": {
        "deviceClasses": [
          {
            "assistantSdkDevice": {}
          }
        ]
      },
      "intent": {
        "name": "com.acme.intents.Room_Number",
        "parameters": [
          {
            "name": "number",
            "type": "SchemaOrg_Number"
          }
        ],
        "trigger": {
          "queryPatterns": [
            "Where's (room)? $SchemaOrg_Number:number",
            "Where is (room)? $SchemaOrg_Number:number",
            "Where's (room)? $SchemaOrg_Number:number at",
            "Where is (room)? $SchemaOrg_Number:number at",
            "How can I find (room)? $SchemaOrg_Number:number",
            "Where can I find (room)? $SchemaOrg_Number:number",
            "Where can I find (room)? $SchemaOrg_Number:number at",
            "Where would I find (room)? $SchemaOrg_Number:number",
            "Where would I find (room)? $SchemaOrg_Number:number at",
            "Show me how to get to (room)? $SchemaOrg_Number:number",
            "How do I get to (room)? $SchemaOrg_Number:number",
            "How can I get to (room)? $SchemaOrg_Number:number",
            "How would I get to (room)? $SchemaOrg_Number:number"
          ]
        }
      },
      "fulfillment": {
        "staticFulfillment": {
          "templatedResponse": {
            "items": [
              {
                "deviceExecution": {
                  "command": "com.acme.commands.Room_Number",
                  "params": {       
                    "number": "$number"
                  }
                }
              }
            ]
          }
        }
      }
    }
  ]
}